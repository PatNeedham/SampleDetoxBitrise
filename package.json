{
  "name": "SampleDetoxBitrise",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-native": "0.56.0"
  },
  "devDependencies": {
    "babel-jest": "23.4.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "^5",
    "detox": "^8.2.2",
    "jest": "23.5.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "16.4.1"
  },
  "detox": {
    "text-runner": "mocha",
    "specs": "e2e",
    "runner-config": "e2e/mocha.opts",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/SampleDetoxBitrise.app",
        "build": "xcodebuild -project ios/SampleDetoxBitrise.xcodeproj -scheme SampleDetoxBitrise -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      }
    }
  },
  "jest": {
    "preset": "react-native"
  }
}
